{
  "name": "celery-plus",
  "version": "0.1.2",
  "description": "A TypeScript implementation of a Celery client for Node.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --write \"{,!(node_modules)/**/}*.{js,ts}\"",
    "format-check": "prettier --check \"{,!(node_modules)/**/}*.{js,ts}\"",
    "dist": "tsup",
    "test": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/mocha/bin/mocha.js --reporter spec --timeout 10000 \"test/**/test_*.ts\"",
    "test:with-deps": "./scripts/test-with-compose.sh",
    "prepublishOnly": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archer947/celery-plus.git"
  },
  "keywords": [
    "node-celery",
    "celery",
    "celery-node",
    "task",
    "worker",
    "queue",
    "background",
    "rabbitmq",
    "redis"
  ],
  "author": "archer947 <emunshi.studio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archer947/celery-plus/issues"
  },
  "homepage": "https://github.com/archer947/celery-plus#readme",
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.2",
    "@types/sinon": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "chai": "^6.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^11.2.0",
    "express": "^5.2.1",
    "mocha": "^11.7.5",
    "prettier": "^3.7.4",
    "sinon": "^21.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "amqplib": "^0.10.9",
    "ioredis": "^5.8.2",
    "uuid": "^13.0.0"
  }
}
